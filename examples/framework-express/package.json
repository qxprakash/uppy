{
  "name": "react-router-uppy-tus-example",
  "version": "1.0.0",
  "private": true,
  "description": "Minimal React Router + Uppy TUS upload example with Express adapter",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "node ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@react-router/express": "latest",
    "@react-router/node": "latest",
    "@tus/file-store": "latest",
    "@tus/server": "latest",
    "@uppy/core": "workspace:*",
    "@uppy/dashboard": "workspace:*",
    "@uppy/react": "workspace:*",
    "@uppy/tus": "workspace:*",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "latest"
  },
  "devDependencies": {
    "@react-router/dev": "latest",
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.6",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
